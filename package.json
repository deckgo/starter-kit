{
  "name": "deckdeckgo-starter",
  "version": "2.1.1",
  "description": "The Progressive Web App alternative for simple presentations",
  "homepage": "https://deckdeckgo.com",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch\" \"npm run dev\"",
    "start-no-remote": "concurrently --kill-others \"npm run watch-no-remote\" \"npm run dev\"",
    "dev": "lite-server",
    "watch": "webpack --watch --mode development",
    "watch-no-remote": "webpack --watch --mode development --env.no-remote=true",
    "build": "webpack --mode production",
    "local": "concurrently --kill-others \"webpack --watch --mode development --env.local=true\" \"npm run dev\""
  },
  "author": {
    "name": "David Dal Busco",
    "email": "david.dalbusco@outlook.com",
    "web": "https://fluster.io"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/deckgo/deckdeckgo-starter.git"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "deckdeckgo-webpack-plugins": "^1.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "lite-server": "^2.5.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "license": "MIT",
  "dependencies": {
    "@deckdeckgo/charts": "^1.0.1",
    "@deckdeckgo/core": "^1.0.1",
    "@deckdeckgo/deck-utils": "^2.1.0",
    "@deckdeckgo/drag-resize-rotate": "^1.0.0",
    "@deckdeckgo/highlight-code": "^1.0.2",
    "@deckdeckgo/lazy-img": "^1.0.0",
    "@deckdeckgo/qrcode": "^1.0.0",
    "@deckdeckgo/remote": "^1.1.0",
    "@deckdeckgo/slide-aspect-ratio": "^1.0.0",
    "@deckdeckgo/slide-author": "^1.0.0",
    "@deckdeckgo/slide-big-img": "^1.0.0",
    "@deckdeckgo/slide-chart": "^1.0.0",
    "@deckdeckgo/slide-code": "^1.0.0",
    "@deckdeckgo/slide-content": "^1.0.0",
    "@deckdeckgo/slide-countdown": "^1.0.0",
    "@deckdeckgo/slide-gif": "^1.0.0",
    "@deckdeckgo/slide-poll": "^1.0.0",
    "@deckdeckgo/slide-qrcode": "^1.0.0",
    "@deckdeckgo/slide-split": "^1.0.0",
    "@deckdeckgo/slide-title": "^1.0.0",
    "@deckdeckgo/slide-video": "^1.0.0",
    "@deckdeckgo/slide-youtube": "^1.0.0",
    "@deckdeckgo/social": "^1.0.0",
    "@deckdeckgo/youtube": "^1.1.0",
    "@ionic/core": "^5.0.7",
    "@webcomponents/custom-elements": "^1.4.1",
    "ionicons": "^5.0.1",
    "web-social-share": "^6.4.0"
  },
  "keywords": [
    "ionic",
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "presentation",
    "slides",
    "slideshow",
    "talk"
  ]
}
